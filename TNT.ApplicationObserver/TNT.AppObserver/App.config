<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
  </startup>
  <appSettings>
    <add key="LogFolder" value="./Logs/"/>
    <!-- List applications -->
    <add key="app.exe" value="app_path"/>
    <add key="notepad.exe" value="C:\Windows\system32\notepad.exe"/>
    <!-- End of list -->
    <add key="SleepMin" value="5"/>
  </appSettings>
</configuration>